{
    "version": "2.0",
    "logging": {
        "logging": {
            "logLevel": {
                "default": "Information",
                "Azure.Core": "Error",
                "Azure.Storage": "Error"
            }
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "includeTypes": "Request,Trace",
                "maxTelemetryItemsPerSecond": 100000
            }
        }
    }
}